{
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "defaultConnection": "Data Source=localhost\\SQLEXPRESS;Initial Catalog=PracticeDB;Integrated Security=True;Encrypt=False;Trust Server Certificate=True"
  },
  "SeriLog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "./Logs/log-.txt",
          "rollingInterval": "Day"
        }
      },
      {
        "Name": "Seq",
        "Args": {
          "serviceUrl": "http://localhost:5341"
        }
      }
    ]
  }
}
